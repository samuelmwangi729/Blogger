    <section class="blog" id="blog">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="pq-section pq-style-1 text-center">
                        <span class="pq-section-sub-title"># our blog</span>
                        <h5 class="pq-section-title" id="blog" data-high_text="Blog"
                            data-title_text="See Our Latest Blog Posts" data-rough_color="#fd4a18"
                            data-rough_type="underline">See Our Latest Blog Posts</h5>
                    </div>
                </div>
                <div class="col-lg-12">
                    <div class="pq-blog">
                        <div class="owl-carousel owl-loaded owl-drag" data-dots="false" data-nav="false"
                            data-desk_num="3" data-lap_num="2" data-tab_num="2" data-mob_num="1" data-mob_sm="1"
                            data-autoplay="true" data-loop="true" data-margin="30">
                            <% if(homeBlogs.length>0){%>
                            <% homeBlogs.forEach(item=>{%>
                            <div class="item">
                                <div class="pq-blog-post">
                                    <div class="pq-post-media">
                                        <img src="<%= `Uploads/${item.FeaturedImage}` %>" alt=" <%= item.Title %>">
                                    </div>
                                    <div class="pq-blog-contain">
                                        <div class="pq-post-meta">
                                            <ul>
                                                <li class="pq-post-author"><i class="fa fa-user"></i>
                                                    <%= moment(item.createdAt, "YYYYMMDD").fromNow(); %>
                                                </li>
                                                <li class="pq-post-tag">
                                                    <a href="/Category/<%= item.blogCategory %>">
                                                        <%= item.blogCategory %>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                        <h5 class="pq-blog-title"><a href="/blog/<%= item.Slug %>">
                                                <%= item.Title %>
                                            </a></h5>
                                        <a href="/blog/<%= item.Slug %>" class="pq-button pq-btn-flat mt-4 mb-2">
                                            <div class="pq-button-block">
                                                <span class="pq-button-text">Read More</span>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <%}) %>
                            <%}else{%>
                            <div class="alert alert-danger">
                                <strong>Error!!</strong> No blogs Present. Please check with us
                                later
                            </div>
                            <%} %>
                        </div>
                    </div>
                </div>
            </div>
